@products.map do |product|
  {
    id: product.id,
    name: product.name,
    price: product.price,
    description: product.description,
    gender: product.gender,
    size: product.size,
    images: product.images,
    image: product.images.first,
    user_id: product.user_id,
    user_address: product.user.address,
  }
end
